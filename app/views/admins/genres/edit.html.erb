<h1>ジャンル編集画面</h1>

<h4>ジャンル</h4>
　<%= form_for(@genre, url: admins_genre_path, method: :patch ) do |f| %>
	<%= f.text_field :name %>
	<%= f.radio_button :status, :true %>
	<%= f.label :status, "有効" %>
	<%= f.radio_button :status, :false %>
	<%= f.label :status, "無効"%>
　　 <%= f.submit "変更を保存する"%>
 <% end %>